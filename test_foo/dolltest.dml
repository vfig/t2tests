DML1

// Object Export of "An Object (2)"

// Note: Reverse links are not exported. Links, receptrons, stims and metaproperties are exported with '++',
//       which will cause duplicates if the DML is used on an already populated object.


// Inventory->Type
+ObjProp 2 "InvType"
{
	"" "Item"
}

// Shape->Model Name
+ObjProp 2 "ModelName"
{
	"" "garcrh"
}

// Position
+ObjProp 2 "Position"
{
	"Location" 4.013, 2.708, 0.000
	"Heading" 0
	"Pitch" 0
	"Bank" 0
}

// Scripts
+ObjProp 2 "Scripts"
{
	"Script 0" "MPDoll"
	"Script 1" ""
	"Script 2" ""
	"Script 3" ""
	"Don't Inherit" FALSE
}

// Engine Features->FrobInfo
+ObjProp 2 "FrobInfo"
{
	"World Action" "[None]"
	"Inv Action" "Script"
	"Tool Action" "[None]"
}

// Inventory->Object Name
+ObjProp 2 "GameName" = "@Name_MPDoll: "[Change Avatar]""

// Inventory->Can't Drop This
+ObjProp 2 "NoDrop"
{
	"" TRUE
}

// Renderer->Has Refs
+ObjProp 2 "HasRefs"
{
	"" FALSE
}

// Networking->Network Category
+ObjProp 2 "NetworkCategory"
{
	"" "Local Only"
}
